{
  "mcpServers": {
    "postgres": {
      "command": "./toolbox",
      "args": ["--prebuilt","postgres","--stdio"],
      "env": {
        "POSTGRES_HOST": "localhost",
        "POSTGRES_PORT": "5434",
        "POSTGRES_DATABASE": "TravelPlanner",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "secret"
      }
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "command": "npx",
      "env": {},
      "type": "stdio"
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    }
  }
}
